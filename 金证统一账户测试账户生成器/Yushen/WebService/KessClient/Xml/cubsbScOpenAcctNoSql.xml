<?xml version="1.0" encoding="utf-8"?>
<!--2.148	券商发起银证开户、预指定和存管签约（仅做业务，不做数据库操作）-->
<request>
  <auth>
    <OPERATOR>31010107</OPERATOR>
  </auth>
  <data>
    <!--操作类型OP_TYPE为0时表示券商发起银证开户一步式，为1时表示券商发起预指定，即两步式中的第一步，为2时BANK_ACCT、FUND_AUTH_DATA、BANK_AUTH_DATA均传空。-->
    <OP_TYPE>0</OP_TYPE>
    <!--货币代码（必传）DD[CURRENCY]-->
    <CURRENCY></CURRENCY>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--资金代码（必传）-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--资产账户属性（非必传）DD[CUACCT_ATTR]-->
    <CUACCT_ATTR></CUACCT_ATTR>
    <!--外部机构（必传）-->
    <EXT_ORG></EXT_ORG>
    <!--KCBP节点（必传）转发节点，具体可在表SUBSYS_CFG中查询-->
    <KCBP_NODE>31</KCBP_NODE>
    <!--内部机构（非必传）-->
    <INT_ORG></INT_ORG>
    <!--证件号码（非必传）-->
    <ID_CODE></ID_CODE>
    <!--客户名称（非必传）-->
    <USER_NAME></USER_NAME>
    <!--外部银行账户（非必传）-->
    <BANK_ACCT></BANK_ACCT>
    <!--存管账户（非必传）-->
    <BDMF_ACCT></BDMF_ACCT>
    <!--资金密码（非必传）-->
    <FUND_AUTH_DATA></FUND_AUTH_DATA>
    <!--银证业务类型-->
    <CUBSB_TYPE>1</CUBSB_TYPE>
    <!--银行密码（非必传）-->
    <BANK_AUTH_DATA></BANK_AUTH_DATA>
    <!--股东账户（可选）-->
    <TRDACCT></TRDACCT>
    <!--银行代码-->
    <BANK_CODE></BANK_CODE>
    <!--签署方式-->
    <SIGN_WAY></SIGN_WAY>
    <!--签署日期-->
    <SIGN_DATE></SIGN_DATE>
    <!--协议版本-->
    <AGREEMENT_VERSION></AGREEMENT_VERSION>
    <!--协议编号-->
    <AGREEMENT_SNO></AGREEMENT_SNO>
    <!--电子证书编号-->
    <AGREEMENT_CA></AGREEMENT_CA>
    <!--协议备注信息-->
    <AGREEMENT_REMARK></AGREEMENT_REMARK>
  </data>
</request>