<?xml version="1.0" encoding="utf-8"?>
<!--2.182	查询客户征信资料
注1：此接口功能需OEM“KBSS融资融券非现场业务”的授权才能使用
-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--申请编号（非必传）-->
    <CDT_APPL_SN></CDT_APPL_SN>
    <!--客户代码（非必传）-->
    <CUST_CODE></CUST_CODE>
    <!--机构代码（非必传）-->
    <INT_ORG></INT_ORG>
    <!--申请类别（复数，以“，”隔开）（非必传）-->
    <APP_SEQ_TYPES></APP_SEQ_TYPES>
    <!--申请状态（复数，以“，”隔开）（非必传）-->
    <CDT_APPL_STATUSES></CDT_APPL_STATUSES>
    <!--调查表编码（非必传）-->
    <SURVEY_SN></SURVEY_SN>
    <!--信用级别编码（非必传）-->
    <CDT_LEVEL_SN></CDT_LEVEL_SN>
    <!--申请日期（非必传）-->
    <CDT_APPL_DATE></CDT_APPL_DATE>
  </data>
</request>