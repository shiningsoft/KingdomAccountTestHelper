<?xml version="1.0" encoding="utf-8"?>
<!--2.247	客户激活标志信息查询-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--开始日期（非必传）-->
    <BGN_DATE></BGN_DATE>
    <!--结束日期（非必传）-->
    <END_DATE></END_DATE>
    <!--机构代码（非必传）-->
