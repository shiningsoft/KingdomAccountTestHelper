    <!--客户标示值（非必传）-->
    <CUST_FLAG_VAL></CUST_FLAG_VAL>
    <!--开户来源（非必传）-->
    <OPEN_SOURCE></OPEN_SOURCE>
    <!--操作标志（非必传）-->
    <OP_FLAG></OP_FLAG>
  </data>
</request>