<?xml version="1.0" encoding="utf-8"?>
<!--2.168	开立信用资产账户
注1：此接口需OEM“KBSS融资融券非现场业务”的授权才能使用。
注2：调用此接口前，请确保两融系统已存在有该客户信息，具体也可通过2.186接口先同步客户资料至两融系统，再开立信用资产账户

-->
<request>
  <auth>
    <OPERATOR>31010013</OPERATOR>
  </auth>
  <data>
    <!--用户代码(必传)-->
    <USER_CODE>310100033013</USER_CODE>
    <!--客户分类DD[CUST_CLS]-->
    <CUST_CLS>0</CUST_CLS>
    <!--内部机构-->
    <INT_ORG>3101</INT_ORG>
    <!--资产账户（非必传）-->
    <CUACCT_CODE>310100033013</CUACCT_CODE>
    <!--资产账户类别（必传）-->
    <CUACCT_CLS>0</CUACCT_CLS>
    <!--资产账户级别（必传）-->
    <CUACCT_LVL>0</CUACCT_LVL>
    <!--资产账户组别（必传）-->
    <CUACCT_GRP>0</CUACCT_GRP>
    <!--货币（必传）DD[CURRENCY]-->
    <CURRENCY>0</CURRENCY>
    <!--帐户类型-->
    <FUND_TYPE>0</FUND_TYPE>
  </data>
</request>