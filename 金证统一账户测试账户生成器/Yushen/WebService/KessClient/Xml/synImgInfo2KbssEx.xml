<?xml version="1.0" encoding="utf-8"?>
<!--2.132	客户影像同步（无关联预约表）
注1：此接口需OEM“KBSS网上开户标准版接口”的授权才能使用。

-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--用户角色DD[USER_ROLE]-->
    <USER_ROLE></USER_ROLE>
    <!--内部机构（必传）-->
    <INT_ORG></INT_ORG>
    <!--证件类型-->
    <ID_TYPE></ID_TYPE>
    <!--证件号码-->
    <ID_CODE></ID_CODE>
    <!--资产账户-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--临柜业务-->
    <FRT_BIZ></FRT_BIZ>
    <!--客户姓名-->
    <CUST_NAME></CUST_NAME>
    <!--业务私有数据-->
    <BIZ_DATA></BIZ_DATA>
    <!--影像编号（以”,”隔开）-->
    <IMG_SNS></IMG_SNS>
    <!--影像类别-->
    <IMG_CLS></IMG_CLS>
    <!--用户代码-->
    <USER_CODE></USER_CODE>
    <!--操作类型-->
    <OPERATOR_TYPE></OPERATOR_TYPE>
    <!--采集标志0-非必要1-必要-->
    <COLL_FLAG></COLL_FLAG>
    <!--手工新增标志0-非手工新增1-手工新增-->
    <HAND_ADD_FLAG></HAND_ADD_FLAG>
    <!--股东账户-->
    <TRDACCT></TRDACCT>
    <!--交易板块DD[STKBD]-->
    <STKBD></STKBD>
    <!--影像备注-->
    <IMG_REMARK></IMG_REMARK>
    <!--影像标签-->
    <IMG_TAG></IMG_TAG>
    <!--影像状态DD[IMG_STATUS]-->
    <IMG_STATUS></IMG_STATUS>
  </data>
</request>