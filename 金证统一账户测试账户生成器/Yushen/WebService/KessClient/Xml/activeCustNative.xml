    <!--资产账户类别（非必传）-->
    <CUACCT_CLS></CUACCT_CLS>
    <!--资产账户级别（非必传）-->
    <CUACCT_LVL></CUACCT_LVL>
    <!--资产账户组别（非必传）-->
    <CUACCT_GRP></CUACCT_GRP>
    <!--币种（非必传）-->
    <CURRENCY></CURRENCY>
    <!--数量（必传）-->
    <AMT></AMT>
    <!--操作备注（非必传）-->
    <OP_REMARK></OP_REMARK>
    <!--激活级别（必传）-->
    <OP_LEVEL></OP_LEVEL>
