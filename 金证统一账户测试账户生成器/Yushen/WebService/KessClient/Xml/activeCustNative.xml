注1：此接口需OEM“KBSS网上开户标准版接口”的授权才能使用。

-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--另库类型（必传）-->
    <IN_STORE_TYPE></IN_STORE_TYPE>
    <!--证券账户类别（必传）-->
