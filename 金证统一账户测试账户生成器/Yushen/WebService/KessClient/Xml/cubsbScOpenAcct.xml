<?xml version="1.0" encoding="utf-8"?>
<!--2.61	券商发起银证开户、预指定和存管签约
注：此接口需OEM“KBSS网上开户标准版接口”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR>31010107</OPERATOR>
  </auth>
  <data>
    <!--操作类型OP_TYPE为0时表示券商发起银证开户一步式，为1时表示券商发起预指定，即两步式中的第一步，为2时BANK_ACCT、FUND_AUTH_DATA、BANK_AUTH_DATA均传空。-->
    <OP_TYPE>0</OP_TYPE>
    <!--货币代码DD[CURRENCY]-->
    <CURRENCY>0</CURRENCY>
    <!--客户代码-->
    <CUST_CODE></CUST_CODE>
    <!--资金代码-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--客户代码-->
    <BANK_CUACCT_CODE></BANK_CUACCT_CODE>
    <!--外部机构-->
    <EXT_ORG></EXT_ORG>
    <!--外部银行账户-->
    <BANK_ACCT></BANK_ACCT>
    <!--资金密码-->
    <FUND_AUTH_DATA></FUND_AUTH_DATA>
    <!--银证业务类型DD[CUBSB_TYPE]-->
    <CUBSB_TYPE>1</CUBSB_TYPE>
    <!--银行密码-->
    <BANK_AUTH_DATA></BANK_AUTH_DATA>
    <!--股东账户（可选）-->
    <TRDACCT></TRDACCT>
    <!--银行代码-->
    <BANK_CODE></BANK_CODE>
    <!--签署方式-->
    <SIGN_WAY></SIGN_WAY>
    <!--签署日期-->
    <SIGN_DATE></SIGN_DATE>
    <!--协议版本-->
    <AGREEMENT_VERSION></AGREEMENT_VERSION>
    <!--协议编号-->
    <AGREEMENT_SNO></AGREEMENT_SNO>
    <!--电子证书编号-->
    <AGREEMENT_CA></AGREEMENT_CA>
    <!--协议备注信息-->
    <AGREEMENT_REMARK></AGREEMENT_REMARK>
    <!--操作类型OP_TYPE为2时表示券商发起存管签约，即两步式中的第二步。-->
    <OP_TYPE>2</OP_TYPE>
    <!--货币类型DD[CURRENCY]-->
    <CURRENCY>0</CURRENCY>
    <!--资金账户-->
    <CUACCT_CODE>310100033019</CUACCT_CODE>
    <!--外部机构-->
    <EXT_ORG></EXT_ORG>
    <!--资金密码-->
    <FUND_AUTH_DATA></FUND_AUTH_DATA>
    <!--银行密码-->
    <BANK_AUTH_DATA></BANK_AUTH_DATA>
    <!--银行账号-->
    <BANK_ACCT></BANK_ACCT>
    <!--存管账户-->
    <BDMF_ACCT></BDMF_ACCT>
    <!--客户代码-->
    <CUST_CODE>310100033019</CUST_CODE>
    <!--客户代码，传客户代码-->
    <RPT_USER_CODE>310100033019</RPT_USER_CODE>
    <!--用户角色DD[USER_ROLE]-->
    <RPT_USER_ROLE>1</RPT_USER_ROLE>
    <!--推荐人编码-->
    <REFE_CODE>1</REFE_CODE>
    <!--银行代码-->
    <BANK_CODE></BANK_CODE>
    <!--签署方式-->
    <SIGN_WAY></SIGN_WAY>
    <!--签署日期-->
    <SIGN_DATE></SIGN_DATE>
    <!--协议版本-->
    <AGREEMENT_VERSION></AGREEMENT_VERSION>
    <!--协议编号-->
    <AGREEMENT_SNO></AGREEMENT_SNO>
    <!--电子证书编号-->
    <AGREEMENT_CA></AGREEMENT_CA>
    <!--协议备注信息-->
    <AGREEMENT_REMARK></AGREEMENT_REMARK>
  </data>
</request>