<?xml version="1.0" encoding="utf-8"?>
<!--2.48	证券账户开户中登处理
注：此接口需OEM“KBSS网上开户标准版接口”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--用户名称-->
    <USER_NAME></USER_NAME>
    <!--用户类型DD[USER_TYPE]-->
    <USER_TYPE></USER_TYPE>
    <!--证件类型-->
    <ID_TYPE></ID_TYPE>
    <!--证件号码-->
    <ID_CODE></ID_CODE>
    <!--用户全称-->
    <USER_FNAME></USER_FNAME>
    <!--证件邮编-->
    <ID_ZIP_CODE></ID_ZIP_CODE>
    <!--证件地址-->
    <ID_ADDR></ID_ADDR>
    <!--证件有效日期-->
    <ID_EXP_DATE></ID_EXP_DATE>
    <!--邮政编码-->
    <ZIP_CODE></ZIP_CODE>
    <!--联系地址-->
    <ADDRESS></ADDRESS>
    <!--联系电话-->
    <TEL></TEL>
    <!--传真电话-->
    <FAX></FAX>
    <!--电子邮箱-->
    <EMAIL></EMAIL>
    <!--移动电话-->
    <MOBILE_TEL></MOBILE_TEL>
    <!--国籍DD[CITIZENSHIP]-->
    <CITIZENSHIP></CITIZENSHIP>
    <!--学历-->
    <EDUCATION></EDUCATION>
    <!--籍贯/注册地-->
    <NATIVE_PLACE></NATIVE_PLACE>
    <!--性别DD[SEX]-->
    <SEX></SEX>
    <!--出生日期/注册日期-->
    <BIRTHDAY></BIRTHDAY>
    <!--职业类型-->
    <OCCU_TYPE></OCCU_TYPE>
    <!--法人代表-->
    <LEGAL_REP></LEGAL_REP>
    <!--法人代表证件号码-->
    <LEGAL_REP_ID_CODE></LEGAL_REP_ID_CODE>
    <!--机构编码-->
    <INT_ORG></INT_ORG>
    <!--客户代码-->
    <CUST_CODE></CUST_CODE>
    <!--开户方式：0柜台开户1客户自助2视频见证3双人见证-->
    <ACCT_OPENTYPE></ACCT_OPENTYPE>
    <!--交易市场开户(预约)-->
    <APPT_OPEN_ACCT></APPT_OPEN_ACCT>
  </data>
</request>