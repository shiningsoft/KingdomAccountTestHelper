<?xml version="1.0" encoding="utf-8"?>
<!--2.105	投资者适当性信息查询-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--客户号（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--内部机构（非必传）-->
    <INT_ORG></INT_ORG>
    <!--用户性质（非必传）DD[USER_TYPE]-->
    <USER_TYPE></USER_TYPE>
    <!--专业投资者分类（非必传）DD[PROF_INVESTOR_TYPE]-->
    <PROF_INVESTOR_TYPE></PROF_INVESTOR_TYPE>
    <!--专业投资者来源（非必传）DD[PROF_INVESTOR_SOURCE]-->
    <PROF_INVESTOR_SOURCE></PROF_INVESTOR_SOURCE>
    <!--投资期限（非必传）DD[INVEST_LMT]-->
    <INVEST_LMT></INVEST_LMT>
    <!--投资品种（非必传）DD[INVEST_PRO]-->
    <INVEST_PRO></INVEST_PRO>
    <!--专业投资者确认开始日期（非必传）-->
    <BGN_PROF_SIGN_DATE></BGN_PROF_SIGN_DATE>
    <!--专业投资者确认结束日期（非必传）-->
    <END_PROF_SIGN_DATE></END_PROF_SIGN_DATE>
    <!--专业投资者到期开始日期（非必传）-->
    <BGN_PROF_EXP_DATE></BGN_PROF_EXP_DATE>
    <!--专业投资者到期结束日期（非必传）-->
    <END_PROF_EXP_DATE></END_PROF_EXP_DATE>
  </data>
</request>