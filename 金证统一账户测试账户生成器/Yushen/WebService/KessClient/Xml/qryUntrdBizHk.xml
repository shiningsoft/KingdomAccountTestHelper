<?xml version="1.0" encoding="utf-8"?>
<!--2.215	非交易报送业务查询（港股）-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--流水序号（非必传）-->
    <SERIAL_NO></SERIAL_NO>
    <!--开始日期（非必传）-->
    <BGN_DATE></BGN_DATE>
    <!--结束日期（非必传）-->
    <END_DATE></END_DATE>
    <!--内部机构（非必传）-->
    <INT_ORG></INT_ORG>
    <!--客户代码（非必传）-->
    <CUST_CODE></CUST_CODE>
    <!--结算账户（非必传）-->
    <SETT_ACCT></SETT_ACCT>
    <!--业务类别（非必传）DD[SZHK_TYPE]-->
    <BIZ_TYPE></BIZ_TYPE>
    <!--委托类型（非必传）DD[SZHK_DLE_TYPE]-->
    <TRUST_TYPE></TRUST_TYPE>
    <!--委托序号（非必传）-->
    <TRUST_SN></TRUST_SN>
    <!--原流水编号（非必传）-->
    <PRV_SERIAL_NO></PRV_SERIAL_NO>
    <!--中登流水号码（非必传）-->
    <ZD_SERIAL_NO></ZD_SERIAL_NO>
    <!--证券代码（非必传）-->
    <STK_CODE></STK_CODE>
    <!--证券账户（非必传）-->
    <TRDACCT></TRDACCT>
    <!--权益类别（非必传）-->
    <ETTLM_TYPE></ETTLM_TYPE>
    <!--上市状态（非必传）-->
    <LIST_STATUS></LIST_STATUS>
    <!--可售标志（非必传）-->
    <SALE_FLAG></SALE_FLAG>
    <!--股份性质（非必传）-->
    <SHARE_ATTR></SHARE_ATTR>
    <!--联动深度（非必传）-->
    <LINK_DEPTH></LINK_DEPTH>
    <!--委托状态（非必传）-->
    <ACCTBIZ_STATUS></ACCTBIZ_STATUS>
    <!--是否查询流水标志（非必传）-->
    <LOG_FLAG></LOG_FLAG>
  </data>
</request>