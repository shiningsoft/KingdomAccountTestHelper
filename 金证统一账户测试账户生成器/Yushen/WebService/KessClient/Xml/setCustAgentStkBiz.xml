<?xml version="1.0" encoding="utf-8"?>
<!--2.114	客户代理关系设置（证券业务）
注：此接口需OEM“KBSS网上开户标准版接口”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--操作类型（必传）（0-增加1-修改2-删除）-->
    <OPERATION_TYPE></OPERATION_TYPE>
    <!--用户代码（必传）-->
    <USER_CODE></USER_CODE>
    <!--用户角色（必传）DD[USER_ROLE]-->
    <USER_ROLE></USER_ROLE>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--开始日期（非必传）-->
    <BGN_DATE></BGN_DATE>
    <!--结束日期（必传）-->
    <END_DATE></END_DATE>
    <!--交易板块（非必传）DD[STKBD]-->
    <STKBD></STKBD>
    <!--交易账户（非必传）-->
    <TRDACCT></TRDACCT>
    <!--证券业务（非必传）-->
    <STK_BIZES></STK_BIZES>
    <!--禁止证券业务（非必传）-->
    <EXCL_STK_BIZES></EXCL_STK_BIZES>
    <!--证券类别（非必传）-->
    <STK_CLSES></STK_CLSES>
    <!--禁止证券类别（非必传）-->
    <EXCL_STK_CLSES></EXCL_STK_CLSES>
  </data>
</request>