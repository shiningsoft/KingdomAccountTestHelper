<?xml version="1.0" encoding="utf-8"?>
<!--2.134	同步风险测评到集中交易系统
注1：此接口需OEM“KBSS网上开户标准版接口”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--操作类型（必输）0-增加1-修改2-删除-->
    <OPERATOR_TYPE></OPERATOR_TYPE>
    <!--用户代码（必输）-->
    <USER_CODE></USER_CODE>
    <!--调查表编码（非必输）-->
    <SURVEY_SN></SURVEY_SN>
    <!--调查表分值（非必输）-->
    <SURVEY_SCORE></SURVEY_SCORE>
    <!--评级级别（必输）-->
    <RATING_LVL></RATING_LVL>
    <!--评级日期（非必输）-->
    <RATING_DATE></RATING_DATE>
    <!--可共用类别（非必输）-->
    <SHARE_SURVEY_CLSES></SHARE_SURVEY_CLSES>
    <!--调查表类别（非必输）-->
    <SURVEY_CLS></SURVEY_CLS>
  </data>
</request>