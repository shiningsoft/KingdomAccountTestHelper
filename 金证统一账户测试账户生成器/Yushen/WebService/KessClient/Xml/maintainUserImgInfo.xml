<?xml version="1.0" encoding="utf-8"?>
<!--2.131	用户影像描述信息操作-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--操作类型（必传）0-增加1-修改2-删除3-修改影像状态5-离柜开户-->
    <OPERATOR_TYPE></OPERATOR_TYPE>
    <!--用户代码（必传）-->
    <USER_CODE></USER_CODE>
    <!--用户角色（必传）DD[USER_ROLE]-->
    <USER_ROLE></USER_ROLE>
    <!--内部机构（非必传）-->
    <INT_ORG></INT_ORG>
    <!--影像类别（必传）-->
    <IMG_CLS></IMG_CLS>
    <!--影像编号-->
    <IMG_SN></IMG_SN>
    <!--影像状态（必传）0-正常DD[IMG_STATUS]-->
    <IMG_STATUS></IMG_STATUS>
    <!--影像标签-->
    <IMG_TAG></IMG_TAG>
    <!--影像备注-->
    <IMG_REMARK></IMG_REMARK>
    <!--任务序号（必传）-->
    <TASK_ID></TASK_ID>
    <!--影像是否需要审核标志0-不需要审核1-需要审核不送默认不需要审核-->
    <AUDIT_FLAG></AUDIT_FLAG>
    <!--板块DD[STKBD]-->
    <STKBD></STKBD>
    <!--证券账户-->
    <TRDACCT></TRDACCT>
    <!--手工新增标志0-非手工新增1-手工新增-->
    <HAND_ADD_FLAG></HAND_ADD_FLAG>
    <!--采集标志0-非必要1-必要-->
    <COLL_FLAG></COLL_FLAG>
    <!--影像条码号-->
    <BARCODES></BARCODES>
    <!--共享影像编号-->
    <SHARE_IMG_SN></SHARE_IMG_SN>
    <!--影像来源0-临柜1-外围-->
    <IMG_SOURCE></IMG_SOURCE>
  </data>
</request>