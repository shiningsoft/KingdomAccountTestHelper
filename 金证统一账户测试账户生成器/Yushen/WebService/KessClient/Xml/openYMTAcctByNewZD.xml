<?xml version="1.0" encoding="utf-8"?>
<!--2.123	开设一码通账户（新）
功能说明：中登开立一码通。
注1：此接口需OEM“KBSS一码通非现场业务”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--新开户标志0-已有一码通1-新开一码通-->
    <NEW_OPEN_FLAG></NEW_OPEN_FLAG>
    <!--流水序号-->
    <SERIAL_NO></SERIAL_NO>
    <!--一码通号-->
    <YMT_CODE></YMT_CODE>
    <!--客户代码-->
    <CUST_CODE></CUST_CODE>
    <!--客户类别（必传）DD[USER_TYPE]-->
    <USER_TYPE></USER_TYPE>
    <!--客户名称（必传）-->
    <CUST_FNAME></CUST_FNAME>
    <!--证件类型（必传）-->
    <ID_TYPE></ID_TYPE>
    <!--证件号码（必传）-->
    <ID_CODE></ID_CODE>
    <!--机构编码（必传）-->
    <INT_ORG></INT_ORG>
    <!--出生日期/注册日期（必传）-->
    <BIRTHDAY></BIRTHDAY>
    <!--证件开始日期（必传）-->
    <ID_BEG_DATE></ID_BEG_DATE>
    <!--证件有效日期（必传）-->
    <ID_EXP_DATE></ID_EXP_DATE>
    <!--国籍（必传）DD[CITIZENSHIP]-->
    <CITIZENSHIP></CITIZENSHIP>
    <!--证件地址（必传）-->
    <ID_ADDR></ID_ADDR>
    <!--联系地址（必传）-->
    <ADDRESS></ADDRESS>
    <!--邮政编码（必传）-->
    <ZIP_CODE></ZIP_CODE>
    <!--职业类型（必传）-->
    <OCCU_TYPE></OCCU_TYPE>
    <!--学历（个人必传）-->
    <EDUCATION></EDUCATION>
    <!--联系电话（必传）-->
    <TEL></TEL>
    <!--移动电话（必传）-->
    <MOBILE_TEL></MOBILE_TEL>
    <!--网络服务（必传）DD[NET_SERVICE]-->
    <NET_SERVICE></NET_SERVICE>
    <!--网络服务密码-->
    <NET_SERVICEPASS></NET_SERVICEPASS>
    <!--性别（必传）DD[SEX]-->
    <SEX></SEX>
    <!--证件类型（机构必传）-->
    <ID_TYPE2></ID_TYPE2>
    <!--组织机构代码证（机构必传）-->
    <ID_CODE2></ID_CODE2>
    <!--有效日期（机构必传）-->
    <ID_EXP_DATE2></ID_EXP_DATE2>
    <!--机构类别（机构必传）-->
    <ORGANIZATION_CLS></ORGANIZATION_CLS>
    <!--资本属性（机构必传）-->
    <CAPITAL_ATTR></CAPITAL_ATTR>
    <!--国有属性（机构必传）-->
    <NATIONAL_ATTR></NATIONAL_ATTR>
    <!--机构简称（机构必传）-->
    <ORG_SIMPLE_NAME></ORG_SIMPLE_NAME>
    <!--法人代表（机构必传）-->
    <LEGAL_REP></LEGAL_REP>
    <!--法人代表证件类型（机构必传）-->
    <LEGAL_REP_ID_TYPE></LEGAL_REP_ID_TYPE>
    <!--法人代表证件代码（机构必传）-->
    <LEGAL_REP_ID_CODE></LEGAL_REP_ID_CODE>
    <!--联系人姓名（机构必传）-->
    <LINKMAN></LINKMAN>
    <!--联系人证件类型（机构必传）-->
    <LINKMAN_ID_TYPE></LINKMAN_ID_TYPE>
    <!--联系人证件代码（机构必传）-->
    <LINKMAN_ID_CODE></LINKMAN_ID_CODE>
    <!--证件2地址（机构必传）-->
    <ID_ADDR2></ID_ADDR2>
    <!--开户方式(必传)DD[ACCT_OPENTYPE]-->
    <ACCT_OPENTYPE></ACCT_OPENTYPE>
  </data>
</request>