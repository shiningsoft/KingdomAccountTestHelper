<?xml version="1.0" encoding="utf-8"?>
<!--2.122	开设中登股东卡（新）
功能说明：中登开立股东账户。
注1：此接口需OEM“KBSS一码通非现场业务”的授权才能使用；
注2：该接口调用时，请确保OPEN_TYPE、ACCT_TYPE和STKBD的关系要对应正确
如要开立深A信用股东卡，则OPEN_TYPE=3，ACCT_TYPE=24，STKBD=00；
注3：若开信用股东账户，需OEM：KBSS融资融券非现场业务 的授权方可使用此接口。
-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--业务类型0-A股账户开户1-基金账户开户2-B股账户开户3-信用账户开户-->
    <OPEN_TYPE></OPEN_TYPE>
    <!--新开户标志0-已有1-新开-->
    <NEW_OPEN_FLAG></NEW_OPEN_FLAG>
    <!--交易板块DD[STKBD]-->
    <STKBD></STKBD>
    <!--客户类别DD[USER_TYPE]-->
    <USER_TYPE></USER_TYPE>
    <!--客户名称-->
    <CUST_FNAME></CUST_FNAME>
    <!--一码通代码（必传）-->
    <YMT_CODE></YMT_CODE>
    <!--证券账户类别-->
    <ACCT_TYPE></ACCT_TYPE>
    <!--证件类型-->
    <ID_TYPE></ID_TYPE>
    <!--证件号码-->
    <ID_CODE></ID_CODE>
    <!--机构编码-->
    <INT_ORG></INT_ORG>
    <!--开户方式DD[ACCT_OPENTYPE]-->
    <ACCT_OPENTYPE></ACCT_OPENTYPE>
    <!--证券账户号码-->
    <TRDACCT></TRDACCT>
    <!--配号证券账户号码沪深A股信用证券账户开户时必填，沪市衍生品合约账户开户时必填，深市衍生品合约账户开户时必填，其他无需填写-->
    <TRDACCT_EX></TRDACCT_EX>
    <!--交易单元-->
    <STKPBU></STKPBU>
    <!--指定结算参与人-->
    <FIRMID></FIRMID>
    <!--国籍DD[CITIZENSHIP]-->
    <CITIZENSHIP></CITIZENSHIP>
    <!--备用字段（非必传）-->
    <REMARK></REMARK>
  </data>
</request>