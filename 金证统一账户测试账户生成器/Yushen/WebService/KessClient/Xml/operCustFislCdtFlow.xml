<?xml version="1.0" encoding="utf-8"?>
<!--2.183	操作客户征信授信流程
注1：此接口功能需OEM“KBSS融资融券非现场业务”的授权才能使用
-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--操作类型（必传）0-增加1-修改2-删除-->
    <OPERATOR_TYPE></OPERATOR_TYPE>
    <!--客户编号（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--流程编号（必传）DD[FLOW_ID]-->
    <FLOW_ID></FLOW_ID>
    <!--申请类别（必传）DD[APP_TYPE]-->
    <APP_TYPE></APP_TYPE>
    <!--关联单据号（必传）-->
    <NOTE_NO></NOTE_NO>
    <!--执行结果状态（必传）DD[EXEC_RESULT]0-已执行成功1-通过2-不通过-->
    <EXEC_RESULT></EXEC_RESULT>
    <!--执行结果说明（必传）-->
    <RESULT_REMARK></RESULT_REMARK>
    <!--备注信息（必传）-->
    <REMARK></REMARK>
    <!--操作备注（必传）-->
    <OP_REMARK></OP_REMARK>
  </data>
</request>