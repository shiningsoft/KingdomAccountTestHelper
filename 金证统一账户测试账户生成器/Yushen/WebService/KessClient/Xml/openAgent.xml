<?xml version="1.0" encoding="utf-8"?>
<!--2.111	代理人开户
注：此接口需OEM“KBSS网上开户标准版接口”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--代理人代码（必传）（0-表示新开代理人）-->
    <AGT_CODE></AGT_CODE>
    <!--用户名称（必传）-->
    <USER_NAME></USER_NAME>
    <!--机构代码（非必传）-->
    <INT_ORG></INT_ORG>
    <!--证件类型（必传）-->
    <ID_TYPE></ID_TYPE>
    <!--证件号码（必传）-->
    <ID_CODE></ID_CODE>
    <!--用户全称（必传）-->
    <USER_FNAME></USER_FNAME>
    <!--发证机关（必传）-->
    <ID_ISS_AGCY></ID_ISS_AGCY>
    <!--证件有效日期（必传）-->
    <ID_EXP_DATE></ID_EXP_DATE>
    <!--证件编码（非必传）-->
    <ID_ZIP_CODE></ID_ZIP_CODE>
    <!--证件地址（非必传）-->
    <ID_ADDR></ID_ADDR>
    <!--邮政编码（必传）-->
    <ZIP_CODE></ZIP_CODE>
    <!--联系地址（必传）-->
    <ADDRESS></ADDRESS>
    <!--联系电话（非必传）-->
    <TEL></TEL>
    <!--传真电话（非必传）-->
    <FAX></FAX>
    <!--电子邮箱（非必传）-->
    <EMAIL></EMAIL>
    <!--移动电话（非必传）-->
    <MOBILE_TEL></MOBILE_TEL>
    <!--国籍（必传）DD[CITIZENSHIP]-->
    <CITIZENSHIP></CITIZENSHIP>
    <!--民族（必传）DD[NATIONALITY]-->
    <NATIONALITY></NATIONALITY>
    <!--学历（必传）-->
    <EDUCATION></EDUCATION>
    <!--籍贯/注册地（必传）-->
    <NATIVE_PLACE></NATIVE_PLACE>
    <!--性别（必传）DD[SEX]-->
    <SEX></SEX>
    <!--出生/注册日期（必传）-->
    <BIRTHDAY></BIRTHDAY>
    <!--备注信息（非必传）-->
    <REMARK></REMARK>
    <!--代理人类别（必传）-->
    <AGT_CLS></AGT_CLS>
    <!--操作渠道（必传）-->
    <CHANNELS></CHANNELS>
    <!--婚姻状况（非必传）-->
    <MARRY></MARRY>
    <!--兴趣爱好（非必传）-->
    <INTEREST></INTEREST>
    <!--交通工具（非必传）DD[VEHICLE]-->
    <VEHICLE></VEHICLE>
    <!--住宅所有权状况（非必传）-->
    <HOUSE_OWNER></HOUSE_OWNER>
    <!--办公电话（非必传）-->
    <OFFICE_TEL></OFFICE_TEL>
    <!--小灵通电话（非必传）-->
    <WELL_TEL></WELL_TEL>
    <!--首选联系电话（必传）-->
    <LINKTEL_ORDER></LINKTEL_ORDER>
    <!--办公地址（非必传）-->
    <OFFICE_ADDR></OFFICE_ADDR>
    <!--公司地址（非必传）-->
    <CORP_ADDR></CORP_ADDR>
    <!--首选联系地址（非必传）-->
    <LINKADDR_ORDER></LINKADDR_ORDER>
  </data>
</request>