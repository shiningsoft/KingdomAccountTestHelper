<?xml version="1.0" encoding="utf-8"?>
<!--2.185	查询客户征信授信流程
注1：此接口功能需OEM“KBSS融资融券非现场业务”的授权才能使用
-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--流程编号（复数，以“，”隔开）（非必传）-->
    <FLOW_IDS></FLOW_IDS>
    <!--申请类型（复数，以“，”隔开）（非必传）-->
    <APP_TYPES></APP_TYPES>
    <!--关联单据号（非必传）-->
    <NOTE_NO></NOTE_NO>
    <!--执行结果状态（非必传）-->
    <EXEC_RESULTS></EXEC_RESULTS>
  </data>
</request>