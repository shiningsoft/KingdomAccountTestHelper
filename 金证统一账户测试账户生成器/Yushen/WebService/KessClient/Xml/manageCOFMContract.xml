<?xml version="1.0" encoding="utf-8"?>
<!--2.219	集合理财合同新增、删除-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--签约行为标志（必传）0－增加/签订，1－修改，2－删除/解约-->
    <ACTION></ACTION>
    <!--用户角色（必传）-->
    <USER_ROLE></USER_ROLE>
    <!--客户编号（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--交易账号（必传）-->
    <OF_TRDACCT></OF_TRDACCT>
    <!--资金帐号（必传）-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--营业厅代码（必传）-->
    <INT_ORG></INT_ORG>
    <!--签约风险提示（必传）-->
    <SIGN_RISK_NOTICE></SIGN_RISK_NOTICE>
    <!--登记公司代码（非必传）-->
    <DBTID></DBTID>
    <!--交易行为（非必传）-->
    <TRADES></TRADES>
    <!--操作方式（非必传）-->
    <CHANNEL></CHANNEL>
    <!--有效期限（非必传）-->
    <LIMIT_DATE></LIMIT_DATE>
    <!--合同签署时间（非必传）-->
    <OCCUR_TIME></OCCUR_TIME>
    <!--备注信息（非必传）-->
    <REMARK></REMARK>
    <!--合同类型（非必传）-->
    <ECTYPE></ECTYPE>
    <!--合同上传标志（非必传）-->
    <STATUS></STATUS>
    <!--风险匹配（非必传）-->
    <RISK_MATCHING></RISK_MATCHING>
    <!--资金保留额度有效期截止日（非必传）-->
    <EXPIRY_DATE></EXPIRY_DATE>
    <!--资产上限（非必传）-->
    <ASSET_FLOOR></ASSET_FLOOR>
    <!--资金保留额度（非必传）-->
    <WORKING_ASSET_FLOOR></WORKING_ASSET_FLOOR>
    <!--资产增值计划协议状态（非必传）-->
    <ASSET_APPR_STATUS></ASSET_APPR_STATUS>
    <!--客户协议暂停有效期限（非必传）-->
    <PAUSE_EXPIRY_DATE></PAUSE_EXPIRY_DATE>
    <!--购买类型（非必传）-->
    <BUY_TYPE></BUY_TYPE>
    <!--基金代码（非必传）-->
    <FUND_CODE></FUND_CODE>
    <!--登记公司代码（非必传）-->
    <TA_CODE></TA_CODE>
    <!--系统标识（非必传）-->
    <SYSTEM_FLAG></SYSTEM_FLAG>
    <!--基金内码（非必传）-->
    <FUND_INTL></FUND_INTL>
    <!--理财协议申报类型（非必传）-->
    <RPT_TYPE></RPT_TYPE>
    <!--用户类型（必传）-->
    <USER_TYPE></USER_TYPE>
  </data>
</request>