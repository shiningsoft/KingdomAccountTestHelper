<?xml version="1.0" encoding="utf-8"?>
<!--2.214	非交易报送业务操作（港股）
注1：此接口需OEM“KBSS深港通”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR>33146</OPERATOR>
  </auth>
  <data>
    <!--内部机构（必传）-->
    <INT_ORG></INT_ORG>
    <!--业务类别（必传）DD[SZHK_TYPE]-->
    <BIZ_TYPE></BIZ_TYPE>
    <!--委托类型（必传）DD[SZHK_DLE_TYPE]-->
    <!--CD-撤单，CX-查询，WT-委托-->
    <TRUST_TYPE></TRUST_TYPE>
    <!--证券代码（必传）-->
    <STK_CODE></STK_CODE>
    <!--证券账户（必传）-->
    <TRDACCT></TRDACCT>
    <!--公司行为代码（必传）-->
    <CRP_ACT_CODE></CRP_ACT_CODE>
