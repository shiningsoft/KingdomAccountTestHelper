    <!--执行机关（非必传）-->
    <EXE_ORG></EXE_ORG>
    <!--股份性质（非必传）-->
    <SHARE_ATTR></SHARE_ATTR>
    <!--冻结期限（非必传）-->
    <FREEZE_MON></FREEZE_MON>
    <!--合同编号（非必传）-->
    <CASE_NO></CASE_NO>
    <!--冻结序号（非必传）-->
    <ORG_FREEZE_SN></ORG_FREEZE_SN>
    <!--联动深度（非必传）-->
    <LINK_DEPTH></LINK_DEPTH>
    <!--申报数量（非必传）-->
    <DECLARE_QTY></DECLARE_QTY>
    <!--委托日期（非必传）-->
    <TRADE_DATE></TRADE_DATE>
    <!--外部序号（非必传）-->
    <EXTERNAL_SN></EXTERNAL_SN>
    <!--通知类别（非必传）-->
    <NOTE_TYPE></NOTE_TYPE>
  </data>
</request>