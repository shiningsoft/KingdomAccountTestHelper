    <!--交易单元（非必传）-->
    <STKPBU></STKPBU>
    <!--权益类别（非必传）-->
    <ETTLM_TYPE></ETTLM_TYPE>
    <!--权益编号（非必传）-->
    <ETTLM_NO></ETTLM_NO>
    <!--公告编号（非必传）-->
    <ANN_NO></ANN_NO>
    <!--议案编号（非必传）-->
    <PRXY_NO></PRXY_NO>
    <!--赞成数量（非必传）-->
    <USER_CODE></USER_CODE>
    <!--反对数量（非必传）-->
    <OPPOSE_QTY></OPPOSE_QTY>
    <!--弃权数量（非必传）-->
    <ABANDON_QTY></ABANDON_QTY>
    <!--对方托管单元（非必传）-->
    <S_STKPBU></S_STKPBU>
