<?xml version="1.0" encoding="utf-8"?>
<!--2.153	客户股票质押式回购申请-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--交易板块（必传）DD-[STKBD]-->
    <STKBD></STKBD>
    <!--交易账号（必传）-->
    <TRDACCT></TRDACCT>
    <!--交易账户名称（非必传）-->
    <TRDACCT_NAME></TRDACCT_NAME>
    <!--资产账户（非必传）-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--融资方向（非必传）DD-[RZFX_TYPE]-->
    <RZFX_TYPE></RZFX_TYPE>
    <!--操作标志（必传）DD-[OPER_FLAG]-->
    <OPER_FLAG></OPER_FLAG>
    <!--协议检查（非必传）-->
    <CHECKFLAG></CHECKFLAG>
    <!--报送状态（必传）DD-[BJHG_RPT_STATUS]-->
    <RPT_STATUS></RPT_STATUS>
    <!--融出类别（非必传）DD-[RCLB_CLS]-->
    <RCLB_CLS></RCLB_CLS>
    <!--特殊标志（非必传）-->
    <SPEC_FLAG></SPEC_FLAG>
    <!--申请日期（非必传）-->
    <APPLY_DATE></APPLY_DATE>
    <!--备注信息（非必传）-->
    <REMARK></REMARK>
  </data>
</request>