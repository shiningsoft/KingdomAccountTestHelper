<?xml version="1.0" encoding="utf-8"?>
<!--2.11	开户进度查询-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--以下为当PRJ_VERSION为0时需要传递的参数列表-->
    <PRJ_VERSION></PRJ_VERSION>
    <!--开始日期-->
    <BGN_DATE></BGN_DATE>
    <!--结束日期-->
    <END_DATE></END_DATE>
    <!--用户类型DD[USER_TYPE]-->
    <USER_TYPE></USER_TYPE>
    <!--预约业务-->
    <APPT_BIZ></APPT_BIZ>
    <!--预约方式-->
    <APPT_CHNL></APPT_CHNL>
    <!--预约状态-->
    <APPT_STATUS></APPT_STATUS>
    <!--复核状态-->
    <CHK_STATUS></CHK_STATUS>
    <!--证件号码-->
    <ID_CODE></ID_CODE>
    <!--机构代码-->
    <INT_ORGS></INT_ORGS>
    <!--派单状态-->
    <ORDER_STATE></ORDER_STATE>
    <!--客户姓名-->
    <CUST_NAME></CUST_NAME>
    <!--客户代码-->
    <CUST_CODE></CUST_CODE>
    <!--资产账号-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--见证工作人员代码-->
    <OP_CODE></OP_CODE>
    <!--见证开户人员代码-->
    <F_USER_CODE></F_USER_CODE>
    <!--付费模式-->
    <PAY_MODE></PAY_MODE>
    <!--审核日期-->
    <CHK_DATE></CHK_DATE>
    <!--审核结果-->
    <CHK_RESULT></CHK_RESULT>
    <!--一审开始时间-->
    <BGN_CHK_ONE_TIME></BGN_CHK_ONE_TIME>
    <!--一审结束时间-->
    <END_CHK_ONE_TIME></END_CHK_ONE_TIME>
    <!--节点状态，复数，用逗号隔开多值。DD[WEB_PHASE]-->
    <WEB_PHASES></WEB_PHASES>
    <!--开户受理开始日期-->
    <BGN_RE_OCCUR_DATE></BGN_RE_OCCUR_DATE>
    <!--开户受理结束日期-->
    <END_RE_OCCUR_DATE></END_RE_OCCUR_DATE>
  </data>
</request>