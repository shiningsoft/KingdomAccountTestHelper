<!--2.179	推荐人信息查询
注1：此接口功能需OEM“KBSS融资融券非现场业务”的授权才能使用
-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--客户代码（非必输）-->
    <CUST_CODE></CUST_CODE>
    <!--推荐人编号（非必输）-->
    <RECM_CODE></RECM_CODE>
    <!--内部机构（非必输）-->
    <INT_ORG></INT_ORG>
  </data>
</request>