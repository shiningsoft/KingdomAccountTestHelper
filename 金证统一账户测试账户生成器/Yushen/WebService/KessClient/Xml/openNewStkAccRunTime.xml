<?xml version="1.0" encoding="utf-8"?>
<!--2.96	证券账户开户新中登处理
功能说明：报送中登开立股东账户，同时加挂至账户系统内。
注1：此接口需OEM“KBSS一码通非现场业务”的授权才能使用。
-->
<request>
  <auth>
    <OPERATOR>31010013</OPERATOR>
  </auth>
  <data>
    <!--业务类型（默认为0）0-A股账户开户1-基金账户开户2-B股账户开户-->
    <OPEN_TYPE></OPEN_TYPE>
    <!--一码通号码（必传）-->
    <YMT_CODE></YMT_CODE>
    <!--客户类别（必传）DD[USER_TYPE]-->
    <USER_TYPE></USER_TYPE>
    <!--客户名称（必传）-->
    <CUST_FNAME></CUST_FNAME>
    <!--证件类型（必传）-->
    <ID_TYPE></ID_TYPE>
    <!--证件号码（必传）-->
    <ID_CODE></ID_CODE>
    <!--机构编码（必传）-->
    <INT_ORG></INT_ORG>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--出生日期/注册日期（必传）-->
    <BIRTHDAY></BIRTHDAY>
    <!--证件开始日期（必传）-->
    <ID_BEG_DATE></ID_BEG_DATE>
    <!--证件有效日期（必传）-->
    <ID_EXP_DATE></ID_EXP_DATE>
    <!--开户板块（必传）DD[STKBD]-->
    <STKBD></STKBD>
    <!--资金账户（必传）-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--交易账户类型（必传）-->
    <TRDACCT_TYPE></TRDACCT_TYPE>
    <!--交易账户类别（必传）-->
    <TRDACCT_EXCLS></TRDACCT_EXCLS>
    <!--交易账户名称（必传）-->
    <TRDACCT_NAME></TRDACCT_NAME>
    <!--国籍（必传）DD[CITIZENSHIP]-->
    <CITIZENSHIP></CITIZENSHIP>
    <!--证件地址（必传）-->
    <ID_ADDR></ID_ADDR>
    <!--联系地址（必传）-->
    <ADDRESS></ADDRESS>
    <!--邮政编码（必传）-->
    <ZIP_CODE></ZIP_CODE>
    <!--职业类型（必传）-->
    <OCCU_TYPE></OCCU_TYPE>
    <!--学历（个人必传）-->
    <EDUCATION></EDUCATION>
    <!--联系电话（必传）-->
    <TEL></TEL>
    <!--移动电话（必传）-->
    <MOBILE_TEL></MOBILE_TEL>
    <!--网络服务（必传）DD[NET_SERVICE]-->
    <NET_SERVICE></NET_SERVICE>
    <!--网络服务密码-->
    <NET_SERVICEPASS></NET_SERVICEPASS>
    <!--性别（必传）-->
    <SEX></SEX>
    <!--开户方式（默认为0）DD[ACCT_OPENTYPE]-->
    <ACCT_OPENTYPE></ACCT_OPENTYPE>
    <!--证件类型（机构必传）-->
    <ID_TYPE2></ID_TYPE2>
    <!--组织机构代码证（机构必传）-->
    <ID_CODE2></ID_CODE2>
    <!--有效日期（机构必传）-->
    <ID_EXP_DATE2></ID_EXP_DATE2>
    <!--机构类别（机构必传）-->
    <ORGANIZATION_CLS></ORGANIZATION_CLS>
    <!--资本属性（机构必传）-->
    <CAPITAL_ATTR></CAPITAL_ATTR>
    <!--国有属性（机构必传）-->
    <NATIONAL_ATTR></NATIONAL_ATTR>
    <!--机构简称（机构必传）-->
    <ORG_SIMPLE_NAME></ORG_SIMPLE_NAME>
    <!--法人代表（机构必传）-->
    <LEGAL_REP></LEGAL_REP>
    <!--法人代表证件类型（机构必传）-->
    <LEGAL_REP_ID_TYPE></LEGAL_REP_ID_TYPE>
    <!--法人代表证件代码（机构必传）-->
    <LEGAL_REP_ID_CODE></LEGAL_REP_ID_CODE>
    <!--联系人姓名（机构必传）-->
    <LINKMAN></LINKMAN>
    <!--联系人证件类型（机构必传）-->
    <LINKMAN_ID_TYPE></LINKMAN_ID_TYPE>
    <!--联系人证件代码（机构必传）-->
    <LINKMAN_ID_CODE></LINKMAN_ID_CODE>
    <!--证件2地址（机构必传）-->
    <ID_ADDR2></ID_ADDR2>
  </data>
</request>