<!--2.168	首次交易日期查询或中登报送-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--客户代码（必输）-->
    <CUST_CODE></CUST_CODE>
    <!--用户类型（非必输）-->
