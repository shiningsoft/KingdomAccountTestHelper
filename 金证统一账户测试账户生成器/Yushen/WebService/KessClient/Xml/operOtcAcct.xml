<?xml version="1.0" encoding="utf-8"?>
<!--2.194	登记账号维护
注：此接口需OEM“KBSSOTC非现场业务”的授权，并且暂时不能使用
-->
<request>
  <auth>
    <OPERATOR></OPERATOR>
  </auth>
  <data>
    <!--操作类型（必传）0-增加1-修改2-删除3-更新4-基金加挂5-基金撤指定-->
    <OPER_TYPE></OPER_TYPE>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--资产账户（必传）-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--登记机构（必传）-->
    <OTC_CODE></OTC_CODE>
    <!--登记账号（非必传）-->
    <OTC_ACCT></OTC_ACCT>
    <!--交易账号（非必传）-->
    <TRANS_ACCT></TRANS_ACCT>
    <!--账户状态（非必传）-->
    <ACCT_STAT></ACCT_STAT>
    <!--同步标志（非必传）1-不同步其它-同步至OTC-->
    <SYN_FLAG></SYN_FLAG>
    <!--产品子类(复数)（非必传）-->
    <INST_CLS></INST_CLS>
    <!--答案串标志（非必传）1-不送其它-送答案串到OTC系统-->
    <GET_SUR_FLAG></GET_SUR_FLAG>
    <!--时间控制标志（非必传）1-不控制其它-控制-->
    <TIME_FLAG></TIME_FLAG>
  </data>
</request>