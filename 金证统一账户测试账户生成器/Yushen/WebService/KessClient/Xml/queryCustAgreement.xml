    <!--资产账户（非必传）-->
    <CUACCT_CODE></CUACCT_CODE>
    <!--交易板块（非必传）-->
    <STKBD></STKBD>
    <!--交易账户（非必传）-->
    <TRDACCT></TRDACCT>
  </data>
</request>