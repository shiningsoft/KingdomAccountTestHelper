<!--2.159	查询客户签署协议-->
<request>
  <auth>
    <OPERATOR>36050</OPERATOR>
  </auth>
  <data>
    <!--客户代码（必传）-->
    <CUST_CODE></CUST_CODE>
    <!--协议类型（非必传）DD[CUST_AGMT_TYPE]若未输则查询全部-->
